{
  "name": "quiet",
  "author": "Stephen McCann",
  "license": "ISC",
  "description": "Menu bar white noise player",
  "main": "main.js",
  "version": "0.1.0",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "productName": "Quiet",
    "appId": "com.electron.quiet",
    "mac": {
