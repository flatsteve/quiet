    "files": [
      "main.js",
      "assets/*.*",
      "package.json"
    ]
  },
  "devDependencies": {
    "electron": "^4.0.1",
    "electron-builder": "^20.38.5"
  },
  "dependencies": {
    "electron-is-dev": "^1.0.1",
    "electron-log": "^2.2.17",
    "play-sound": "^1.1.3"
  }
}
